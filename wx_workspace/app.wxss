page {
  background-color: #F8F8F8;
  height: 100%;
  font-size: 32rpx;
  line-height: 1.6;
}

.page-body {
  padding: 20rpx 0;
}

.btn-area{
  margin-top: 60rpx;
  box-sizing: border-box;
  width: 100%;
  padding: 0 30rpx;
}

.tc{
  text-align: center;
}
/**app.wxss**/
.container {
  width: 100%;
  height: 100%;
  background-color: #f3f3f3;
} 
/**footer.wxss**/
.footer{
    position: fixed;
    z-index: 888;
    bottom: 0;
    right: 0;
    left: 0;
    display: table;
    width: 100%;
    height: 50px;
    padding-right: 10px;
    padding-left: 10px;
    padding: 0;
    box-shadow: 0 -1px 0 0 rgba(0,0,0,.03);
    background-color: #00b26a;
    -webkit-backdrop-filter: blur(10px);
}
.tab-bar{
    height: 50px;
    display: -webkit-box;
}
.tab-bar view{
    width: 25%;
    overflow: hidden;
    height: 50px;
    line-height: 50px;
    text-align: center;
    color: #fff;
    font-size: 15px;
}
.tab-bar .is_checked{  
  border: 1px solid #FE0002 ;  
  color: #FE0002 ;  
  background: #fff;  
}  
.tab-bar .normal{  
  border: none;  
  color: #333;  
  background: #F0F1EC;  
} 
/**nav.wxss**/
.nav{
    position: fixed;
    top: 0;
    padding: 8px 0 0 20px;
    height : auto;
    box-sizing: border-box;
    z-index: 888;
}
.nav image{
  width: 44px;
  height: 42px;
}
.clearfix:after {
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}